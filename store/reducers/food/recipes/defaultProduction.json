        "56f61031-4e16-47c3-ae87-43280b746a9f": {
            "uuid": "56f61031-4e16-47c3-ae87-43280b746a9f",
            "name": "Vietnamese Pho",
            "description": "A Vietnamese based broth dish.",
            "images": [
                {
                    "key": "31ae6d9d-60f4-46cb-bddc-7fd2810d7e7d/images/fea5cb86-cc71-4504-9ee9-2e928f79f930_Vietnamese-Pho-Recipe-Beef-Chicken-Pork-Shrimp-2.jpg",
                    "timestamp": 1650281482472
                }
            ],
            "components": [
                {
                    "name": "Broth",
                    "ingredients": [
                        {
                            "name": "White onion",
                            "quantity": {
                                "unit": "quantity",
                                "value": 1
                            }
                        },
                        {
                            "name": "Fresh ginger",
                            "quantity": {
                                "unit": "g",
                                "value": 20
                            }
                        },
                        {
                            "name": "Star anise",
                            "quantity": {
                                "unit": "quantity",
                                "value": 5
                            }
                        },
                        {
                            "name": "Whole cloves",
                            "quantity": {
                                "unit": "quantity",
                                "value": 4
                            }
                        },
                        {
                            "name": "Cinnamon sticks",
                            "quantity": {
                                "unit": "quantity",
                                "value": 3
                            }
                        },
                        {
                            "name": "Cardamom pods",
                            "quantity": {
                                "unit": "quantity",
                                "value": 2
                            }
                        },
                        {
                            "name": "Whole coriander seeds",
                            "quantity": {
                                "unit": "tbsp",
                                "value": 1
                            }
                        },
                        {
                            "name": "Beef stock",
                            "quantity": {
                                "unit": "cup",
                                "value": 8
                            }
                        },
                        {
                            "name": "Brown sugar",
                            "quantity": {
                                "unit": "tbsp",
                                "value": 1
                            }
                        },
                        {
                            "name": "Fish sauce",
                            "quantity": {
                                "unit": "tsp",
                                "value": 2
                            }
                        },
                        {
                            "name": "Sea salt",
                            "quantity": {
                                "unit": "none"
                            }
                        }
                    ],
                    "instructions": [
                        {
                            "text": "Turn the oven broiler to high, and place the baking rack about 8 inches away from the heating elements.  Place the onion and ginger cut-side-up on a baking sheet, and brush with a bit of oil.  Broil for about 7-10 minutes, until the tops of the onion and ginger are slightly charred.  Remove and set aside."
                        },
                        {
                            "text": "Meanwhile, heat the anise, cloves, cinnamon, cardamom and coriander to a large stockpot over medium-high heat for about 3 minutes until fragrant. "
                        },
                        {
                            "text": "Add in the charred onion, ginger, stock, and stir to combine.  Continue cooking until the broth reaches a simmer.  Then reduce heat to medium-low, cover with a lid, and continue to simmer for at least 30 minutes."
                        },
                        {
                            "text": "Strain out (and discard) the onions, ginger and spices.  Stir in the fish sauce and sweetener into the hot broth.  Then finally, taste and season the broth with salt as needed."
                        }
                    ],
                    "storeable": true,
                    "servings": 5,
                    "uuid": "10cd9ef9-0b93-4c79-b58d-3d58f43dab09"
