                        },
                        {
                            "name": "Fish sauce",
                            "quantity": {
                                "unit": "tbsp",
                                "value": 1.5
                            }
                        },
                        {
                            "name": "Lime juice",
                            "quantity": {
                                "unit": "tbsp",
                                "value": 2
                            }
                        },
                        {
                            "name": "Brown sugar",
                            "quantity": {
