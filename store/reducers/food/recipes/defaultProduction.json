        "967e8b77-1d04-4b3c-9b58-c750ccb3b1dc": {
            "uuid": "967e8b77-1d04-4b3c-9b58-c750ccb3b1dc",
            "name": "Vegetable fried rice",
            "description": "",
            "images": [
                {
                    "key": "31ae6d9d-60f4-46cb-bddc-7fd2810d7e7d/images/adfd15f4-fdee-424b-b525-b95b6a496a6f_vegetable-fried-rice-550x757.jpg",
                    "timestamp": 1650401671189
                }
            ],
            "components": [
                {
                    "name": "",
                    "ingredients": [
                        {
                            "name": "Avocado oil",
                            "quantity": {
                                "unit": "tbsp",
                                "value": 2
                            }
                        },
                        {
                            "name": "Eggs",
                            "quantity": {
                                "unit": "quantity",
                                "value": 2
                            }
                        },
                        {
                            "name": "Small white onion",
                            "quantity": {
                                "unit": "quantity",
                                "value": 1
                            }
                        },
                        {
                            "name": "Medium carrots",
                            "quantity": {
                                "unit": "quantity",
